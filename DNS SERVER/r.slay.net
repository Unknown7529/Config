;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	slay.net. root.slay.net. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.slay.net.
ns	IN	A	10.10.10.1
@	IN	A	10.10.10.1
1	IN	PTR	ns.slay.net.
1	IN	PTR	www.slay.net.
1	IN	PTR	mail.slay.net.
1	IN	PTR	sftp.slay.net.
